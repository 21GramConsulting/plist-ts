{
  "name": "@adam-rocska/plist",
  "version": "1.0.0",
  "description": "This library provides a parser with which you may parse PLIST files to JavaScript objects.",
  "main": "./plist/dist/index.js",
  "module": "./plist/dist/index.esm.js",
  "types": "./plist/dist/index.d.ts",
  "typesVersions": {
    "*": {
      "old-style": [
        "./old-style/dist/index.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./plist/dist/index.mjs",
      "module": "./plist/dist/index.esm.js",
      "require": "./plist/dist/index.js"
    },
    "./old-style": {
      "import": "./old-style/dist/index.mjs",
      "module": "./old-style/dist/index.esm.js",
      "require": "./old-style/dist/index.js"
    }
  },
  "files": [
    "plist/dist/**/*.{js,d.ts,mjs,d.mts}",
    "old-style/dist/**/*.{js,d.ts,mjs,d.mts}",
    "plist/package.json",
    "old-style/package.json"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.11.0"
  },
  "scripts": {
    "csb:install": "corepack enable && corepack pnpm i",
    "csb:build": "pnpm build",
    "clean": "pnpm -r run clean && rimraf ./var",
    "build": "pnpm -r run build",
    "attw": "attw --pack",
    "types:check": "pnpm -r run types:check",
    "lint": "eslint .",
    "prepublishOnly": "set -e && pnpm types:check && pnpm lint && pnpm attw"
  },
  "homepage": "https://adam-rocska.github.io/plist-ts/",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adam-rocska/plist-ts.git"
  },
  "bugs": {
    "url": "https://github.com/adam-rocska/plist-ts/issues"
  },
  "keywords": [
    "plist",
    "old-style",
    "plist",
    "plist-ts"
  ],
  "author": {
    "name": "Adam Laszlo Rocska",
    "url": "https://github.com/adam-rocska"
  },
  "contributors": [ ],
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.13.4",
    "@swc/core": "^1.3.101",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "bunchee": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "microbundle": "^0.15.1",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "typescript": "5.3.3",
    "opentype.js": "^1.3.4",
    "@types/opentype.js": "^1.3.8"
  }
}
